	<test name="UnitLevel">
		<classes>
			<class name="com.comarch.training.FailingCases" />
		</classes>
	</test>



	<test name="AcceptanceLevel">
		<classes>
			<class
				name="com.comarch.training.FailingCases" />
		</classes>
	</test>


	<test name="IntegrationLevel">
		<classes>
			<class
				name="com.comarch.training.FailingCases" />
		</classes>
	</test>


</suite>